{
	"name": "@sv/components",
	"version": "0.3.1",
	"author": "t.havlicek@s-v.de",
	"type": "module",
	"contributors": [],
	"bin": "./cli.js",
	"scripts": {
		"build:darwin:arm": "GOOS=darwin GOARCH=arm64 go build -ldflags='-s -w' -o ./dist/aarch64-darwin/components ./src/cli",
		"build:darwin:x64": "GOOS=darwin GOARCH=amd64 go build -ldflags='-s -w' -o ./dist/x64-darwin/components ./src/cli",
		"build:linux:x64": "GOOS=linux GOARCH=amd64 go build -ldflags='-s -w' -o ./dist/x64-linux/components ./src/cli",
		"build": "bun run build:darwin:arm && bun run build:darwin:x64 && bun run build:linux:x64 && chmod -R +x ./dist",
		"test": "bun test"
	},
	"files": [
		"dist",
		"templates",
		"./use.js"
	]
}
