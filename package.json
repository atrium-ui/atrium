    },
    "./adaptive": {
      "import": "./packages/adaptive/dist/index.js",
      "types": "./packages/adaptive/src/index.ts"
    },
    "./toggle": {
      "import": "./packages/toggle/dist/index.js",
      "types": "./packages/toggle/src/index.ts"
    },
    "./expandable": {
      "import": "./packages/expandable/dist/index.js",
      "types": "./packages/expandable/src/index.ts"
    },
    "./dropdown": {
      "import": "./packages/dropdown/dist/index.js",
      "types": "./packages/dropdown/src/index.ts"
    },
    "./circle": {
      "import": "./packages/circle/dist/index.js",
      "types": "./packages/circle/src/index.ts"
    },
    "./pager": {
      "import": "./packages/pager/dist/index.js",
      "types": "./packages/pager/src/index.ts"
    },
    "./track": {
      "import": "./packages/track/dist/index.js",
      "types": "./packages/track/src/index.ts"
    },
