---
import { getEntry } from "astro:content";
import { render } from "astro:content";
import Layout from "../layout.astro";

const entry = await getEntry("docs", "index");

if (!entry) {
  throw new Error("404");
}

const { Content, headings, remarkPluginFrontmatter } = await render(entry);
---

<Layout>
  <Content />
</Layout>
