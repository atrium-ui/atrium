{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": [
    "astro/tsconfigs/strict",
    "@atrium-ui/codestyle/tsconfig.json"
  ],
  "compilerOptions": {
    "paths": {
      "package:/components": [
        "./src/components"
      ],
      "@components/*": [
        "../packages/components/*"
      ]
    },
    "jsx": "preserve",
    "types": ["vite/client"]
  }
}
